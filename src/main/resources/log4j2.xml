<?xml version="1.0" encoding="UTF-8"?>

<Configuration status="INFO">


    -<Appenders>


    -<File append="false" immediateFlush="true" fileName="./src/main/resources/program.log" name="FileAppender">

    <PatternLayout pattern="%d{yyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>

</File>

</Appenders>


    -<Loggers>


    -<Root level="ALL">

    <AppenderRef ref="FileAppender"/>

</Root>

</Loggers>

</Configuration>